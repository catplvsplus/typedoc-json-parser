{
  "name": "@catplvsplus/typedoc-json-parser",
  "version": "10.2.2",
  "type": "module",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "check": "tsc --noEmit",
    "docs": "tsdown && bun dist/bin/index.js --json docs/api.json --verbose",
    "prepack": "bun run build"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^25.0.8",
    "rolldown": "^1.0.0-beta.60",
    "tsdown": "^0.20.0-beta.3",
    "typescript": "^5.5.0"
  },
  "dependencies": {
    "pathe": "^2.0.3"
  },
  "peerDependencies": {
    "typedoc": "^0.26.11"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "bun@1.2.18"
}
